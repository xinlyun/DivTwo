<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:fitsSystemWindows="true"

    android:clipToPadding="false"
    android:background="#c0c0c0"

    android:layout_height="match_parent">
    <include layout="@layout/map_return_bar"/>
    <android.support.v7.widget.CardView
        android:background="@drawable/route_long_scene_bg"
        card_view:cardElevation="5dp"
        card_view:cardMaxElevation="10dp"
        card_view:cardCornerRadius="10dp"
        card_view:cardUseCompatPadding="true"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="25dp"
        android:layout_height="wrap_content">
    <LinearLayout
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="166dp">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="140dp"
                android:layout_height="match_parent">
                <ImageView
                    android:layout_marginTop="24dp"
                    android:src="@drawable/c3_03"
                    android:layout_gravity="center_horizontal"
                    android:layout_weight="1"
                    android:layout_width="20dp"
                    android:layout_height="20dp" />
                <ImageView
                    android:layout_marginTop="4dp"
                    android:src="@drawable/c2_07"
                    android:layout_weight="1"
                    android:layout_gravity="center_horizontal"
                    android:layout_width="38dp"
                    android:layout_height="38dp" />
                <ImageView
                    android:src="@drawable/c1_11"
                    android:layout_weight="1"
                    android:layout_marginBottom="24dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_width="20dp"
                    android:layout_height="20dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_height="wrap_content">
                <TextView
                    android:textSize="30dp"
                    android:text="   当前位置"
                    android:textColor="#009cff"
                    android:gravity="center_vertical"
                    android:id="@+id/naviprepare_nowposi"
                    android:layout_width="match_parent"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_height="82dp" />
                <ImageView
                    android:background="#c9c9c9"
                    android:layout_width="match_parent"
                    android:layout_height="2dp" />
                <TextView
                    android:textSize="30dp"
                    android:id="@+id/naviprepare_destination"
                    android:text="   终点"
                    android:textColor="#888687"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="82dp" />

            </LinearLayout>

        </LinearLayout>
        <ImageView
            android:background="#c9c9c9"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_width="match_parent"
            android:layout_height="2dp" />
        <LinearLayout
            android:layout_marginLeft="100dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_marginTop="10dp"
                android:id="@+id/navi_strategy_edit"
                style="@style/Widget.AppCompat.AutoCompleteTextView"

                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:gravity="center"
                android:focusable="false"
                android:layout_weight="1"
                android:text="路线偏好"
                android:textColor="#888687"
                android:textSize="30dp" >
            </TextView>
            <ImageView
                android:src="@drawable/set_03"
                android:layout_weight="7.5"
                android:layout_gravity="center"
                android:gravity="center"
                android:id="@+id/naviprepare_celue_set"
                android:layout_width="match_parent"
                android:layout_height="72dp" />
        </LinearLayout>


        <!--<ImageView-->
            <!--android:background="#c9c9c9"-->
            <!--android:layout_marginLeft="10dp"-->
            <!--android:layout_marginRight="10dp"-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="2dp" />-->
        <Button
            android:id="@+id/naviprepare_goon"
            android:text="出发"
            android:textSize="30dp"
            android:background="#657992"
            android:textColor="#60000000"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_width="match_parent"
            android:layout_height="72dp" />
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="18dp" />
    </LinearLayout>
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:background="@drawable/route_long_scene_bg"
        card_view:cardElevation="5dp"
        card_view:cardMaxElevation="10dp"
        card_view:cardCornerRadius="10dp"
        card_view:cardUseCompatPadding="true"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="25dp"
        android:layout_height="wrap_content">
    <LinearLayout
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:background="#99ffffff"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="100dp"
            android:layout_marginRight="10dp"
            android:layout_height="72dp">
            <TextView
                android:textSize="30dp"
                android:text="家"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:id="@+id/naviprepare_home"
                android:layout_width="match_parent"
                android:layout_weight="1"


                android:layout_height="72dp" />

            <ImageView
                android:src="@drawable/set_03"
                android:layout_weight="7.5"
                android:layout_gravity="center"
                android:gravity="center"
                android:id="@+id/naviprepare_home_set"
                android:layout_width="match_parent"
                android:layout_height="72dp" />

        </LinearLayout>
        <ImageView
            android:background="#c9c9c9"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_width="match_parent"
            android:layout_height="2dp" />
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="100dp"
            android:layout_marginRight="10dp"
            android:layout_height="72dp">
            <TextView
                android:textSize="30dp"
                android:text="公司"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical"
                android:id="@+id/naviprepare_company"
                android:layout_width="match_parent"
                android:layout_weight="1"


                android:layout_height="115dp" />

            <ImageView
                android:src="@drawable/set_03"
                android:layout_weight="7.5"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:id="@+id/naviprepare_company_set"
                android:layout_width="match_parent"
                android:layout_height="72dp" />
        </LinearLayout>
        <ImageView
            android:background="#c9c9c9"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_width="match_parent"
            android:layout_height="2dp" />
        <ListView
            android:layout_marginStart="100dp"
            android:id="@+id/naviprepare_history"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        </ListView>
    </LinearLayout>
    <ImageView
        android:background="#c9c9c9"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="2dp" />
    <Button
        android:id="@+id/naviprepare_cleanhistory"
        android:text="清除历史信息"
        android:textSize="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_width="match_parent"
        android:layout_height="82dp" />
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="18dp" />
    </LinearLayout>
    </android.support.v7.widget.CardView>
</LinearLayout>