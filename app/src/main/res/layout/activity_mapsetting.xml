<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:background="#c0c0c0"
    android:fitsSystemWindows="true"
    android:clipToPadding="false"
    android:layout_height="match_parent">
    <include layout="@layout/map_return_bar"/>
    <android.support.v7.widget.CardView
        android:background="@drawable/route_long_scene_bg"
        card_view:cardElevation="5dp"
        card_view:cardMaxElevation="10dp"
        card_view:cardCornerRadius="10dp"
        card_view:cardUseCompatPadding="true"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="25dp"
        android:layout_height="324dp">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout

                android:layout_marginRight="40dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="100dp">
                
                <ImageView
                    android:src="@drawable/life"
                    
                    android:layout_width="50dp"
                    android:layout_marginLeft="25dp"
                    android:layout_height="match_parent" />
                <TextView
                    android:layout_marginLeft="25dp"
                    android:id="@+id/map_setting_in1"
                    android:gravity="center_vertical"
                    android:textSize="40dp"
                    android:layout_weight="1"
                    android:text="地图管理"
                    android:background="@drawable/layout_touch_bg"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />


            </LinearLayout>
            <ImageView
                android:background="#50000000"
                android:layout_marginLeft="50dp"
                android:layout_marginRight="50dp"
                android:layout_width="match_parent"
                android:layout_height="0dp" />
            <LinearLayout
                android:layout_marginRight="40dp"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="100dp">
                <ImageView
                    android:src="@drawable/list_normal"
                    
                    android:layout_marginLeft="25dp"
                    android:layout_width="50dp"
                    android:layout_height="match_parent" />
                <TextView
                    android:gravity="center_vertical"
                    android:textSize="40dp"
                    android:layout_marginLeft="25dp"
                    android:layout_weight="1"
                    android:text="街区详图"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <Switch
                    style="@style/widget_gender_switch"
                    android:layout_gravity="center"
                    android:layout_weight="5"
                    android:id="@+id/map_setting_switch1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>
            <ImageView
                android:layout_marginLeft="50dp"
                android:layout_marginRight="50dp"
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="0dp" />
            <LinearLayout
                android:layout_marginRight="40dp"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:background="@drawable/layout_touch_bg"
                android:layout_height="100dp">
                <ImageView
                    android:src="@drawable/icon_scenic_default"
                    android:layout_width="50dp"
                    android:layout_marginLeft="25dp"
                    android:layout_height="match_parent" />
                <TextView
                    android:layout_marginLeft="25dp"
                    android:gravity="center_vertical"
                    android:textSize="40dp"
                    android:layout_weight="1"
                    android:text="地点优先显示"
                    android:id="@+id/map_setting_in2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

            </LinearLayout>
        </LinearLayout>
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        card_view:cardUseCompatPadding="true"
        card_view:cardElevation="5dp"
        card_view:cardMaxElevation="10dp"
        card_view:cardCornerRadius="10dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="25dp"
        android:layout_height="726dp">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_marginRight="40dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="100dp">
                <ImageView
                    android:src="@drawable/navitts_record_pressed"
                    android:layout_width="50dp"
                    android:layout_marginLeft="25dp"
                    android:layout_height="match_parent" />
                <TextView
                    android:layout_marginLeft="25dp"
                    android:gravity="center_vertical"
                    android:textSize="40dp"
                    android:layout_weight="1"
                    android:text="语音"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <Switch
                    style="@style/widget_gender_switch"
                    android:layout_gravity="center"
                    android:layout_weight="5"
                    android:id="@+id/map_setting_switch2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>
            <ImageView
                android:layout_marginLeft="50dp"
                android:layout_marginRight="50dp"
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="0dp" />
            <LinearLayout
                android:layout_marginRight="40dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="100dp">
                <ImageView
                     
                    android:src="@drawable/mine_home_avatar_notlogin"
                    android:layout_width="50dp"
                    android:layout_marginLeft="25dp"
                    android:layout_height="match_parent" />
                <TextView
                    android:layout_marginLeft="25dp"
                    android:gravity="center_vertical"
                    android:textSize="40dp"
                    android:layout_weight="1"
                    android:text="语音类型"
                    android:id="@+id/map_setting_in3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

            </LinearLayout>
            <ImageView
                android:layout_marginLeft="50dp"
                android:layout_marginRight="50dp"
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="0dp" />
            <LinearLayout
                android:layout_marginRight="40dp"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="100dp">
                <ImageView
                     
                    android:src="@drawable/autonavi_view_traffic"
                    android:layout_width="50dp"
                    android:layout_marginLeft="25dp"
                    android:layout_height="match_parent" />
                <TextView
                    android:layout_marginLeft="25dp"
                    android:gravity="center_vertical"
                    android:textSize="40dp"
                    android:layout_weight="1"
                    android:text="路况播报"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <Switch

                    android:layout_gravity="center"
                    android:layout_weight="5"
                    android:id="@+id/map_setting_switch3"
                    android:layout_width="match_parent"
                    style="@style/widget_gender_switch"
                    android:layout_height="match_parent" />
            </LinearLayout>
            <ImageView
                android:layout_marginLeft="50dp"
                android:layout_marginRight="50dp"
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="0dp" />
            <LinearLayout
                android:layout_marginRight="40dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="100dp">
                <ImageView
                    android:src="@drawable/traffic_report_danger"
                    android:layout_width="50dp"
                     
                    android:layout_marginLeft="25dp"
                    android:layout_height="match_parent" />
                <TextView
                    android:layout_marginLeft="25dp"
                    android:gravity="center_vertical"
                    android:textSize="40dp"
                    android:layout_weight="1"
                    android:text="语音提示频率"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" />
                <LinearLayout
                    android:orientation="horizontal"
                    android:background="@drawable/kuang2_23"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                    <Button
                        android:layout_weight="1"
                        android:textSize="30dp"
                        android:background="#888687"
                        android:text="频繁"
                        android:gravity="center"
                        android:id="@+id/map_setting_switch4_true"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" />
                    <Button
                        android:id="@+id/map_setting_switch4_false"
                        android:textSize="30dp"
                        android:background="#00000000"
                        android:textColor="#888687"
                        android:layout_weight="1"
                        android:text="一般"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" />
                </LinearLayout>
            </LinearLayout>
            <ImageView
                android:layout_marginLeft="50dp"
                android:layout_marginRight="50dp"
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="0dp" />
            <LinearLayout
                android:layout_marginRight="40dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="100dp">
                <ImageView
                     
                    android:src="@drawable/traffic_report_police"
                    android:layout_width="50dp"
                    android:layout_marginLeft="25dp"
                    android:layout_height="match_parent" />
                <TextView
                    android:layout_marginLeft="25dp"
                    android:gravity="center_vertical"
                    android:textSize="40dp"
                    android:layout_weight="1"
                    android:text="电子狗播报"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <Switch
                    style="@style/widget_gender_switch"
                    android:layout_gravity="center"
                    android:layout_weight="5"
                    android:id="@+id/map_setting_switch5"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>
            <ImageView
                android:layout_marginLeft="50dp"
                android:layout_marginRight="50dp"
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="0dp" />
            <LinearLayout
                android:layout_marginRight="40dp"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="100dp">
                <ImageView
                     
                    android:src="@drawable/traffic_report_announcement"
                    android:layout_width="50dp"
                    android:layout_marginLeft="25dp"
                    android:layout_height="match_parent" />
                <TextView
                    android:layout_marginLeft="25dp"
                    android:gravity="center_vertical"
                    android:textSize="40dp"
                    android:layout_weight="1"
                    android:text="欢迎语"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <Switch
                    android:layout_gravity="center"
                    android:layout_weight="5"
                    style="@style/widget_gender_switch"
                    android:id="@+id/map_setting_switch6"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>
            <ImageView
                android:layout_marginLeft="50dp"
                android:layout_marginRight="50dp"
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="0dp" />
            <LinearLayout
                android:layout_marginRight="40dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="100dp">
                <ImageView
                     
                    android:src="@drawable/audio_guide_map_icon_focus"
                    android:layout_width="50dp"
                    android:layout_marginLeft="25dp"
                    android:layout_height="match_parent" />
                <TextView
                    android:layout_marginLeft="25dp"
                    android:gravity="center_vertical"
                    android:textSize="40dp"
                    android:layout_weight="1"
                    android:text="语音选择"
                    android:id="@+id/map_setting_in4"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

            </LinearLayout>
        </LinearLayout>
    </android.support.v7.widget.CardView>



    <android.support.v7.widget.CardView
        card_view:cardUseCompatPadding="true"
        card_view:cardElevation="5dp"
        card_view:cardMaxElevation="10dp"
        card_view:cardCornerRadius="10dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="25dp"
        android:layout_height="222dp">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_marginRight="40dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="100dp">
                <ImageView
                     
                    android:src="@drawable/e_dog_radar"
                    android:layout_width="50dp"
                    android:layout_marginLeft="25dp"
                    android:layout_height="match_parent" />
                <TextView
                    android:layout_marginLeft="25dp"
                    android:gravity="center_vertical"
                    android:textSize="40dp"
                    android:layout_weight="1"
                    android:text="优先联网搜索"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <Switch
                    style="@style/widget_gender_switch"
                    android:layout_gravity="center"
                    android:layout_weight="5"
                    android:id="@+id/map_setting_switch7"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>
            <ImageView
                android:layout_marginLeft="50dp"
                android:layout_marginRight="50dp"
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="0dp" />
            <LinearLayout
                android:layout_marginRight="40dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="100dp">
                <ImageView
                     
                    android:src="@drawable/groupbuy_travel"
                    android:layout_width="50dp"
                    android:layout_marginLeft="25dp"
                    android:layout_height="match_parent" />
                <TextView
                    android:layout_marginLeft="25dp"
                    android:gravity="center_vertical"
                    android:textSize="40dp"
                    android:layout_weight="1"
                    android:text="地点即时显示"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <Switch
                    style="@style/widget_gender_switch"
                    android:layout_gravity="center"
                    android:layout_weight="5"
                    android:id="@+id/map_setting_switch8"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />
            </LinearLayout>
        </LinearLayout>
    </android.support.v7.widget.CardView>


</LinearLayout>